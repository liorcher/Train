{
    "name": "trainserver",
    "version": "1.0.0",
    "description": "train project colman",
    "main": "app.js",
    "scripts": {
        "start": "tsc && node dist/server.js",
        "dev": "ts-node-dev --env-file=.env --respawn --transpile-only ./server.ts",
        "test": "tsc && jest --detectOpenHandles --forceExit --coverage",
        "testAuth": "tsc && jest --detectOpenHandles --forceExit --coverage ./tests/auth.test.ts",
        "prod": "cp .env.prod .env && tsc -p tsconfig.prod.json && node dist/server.js",
        "prodPM2": "cp .env.prod .env && tsc -p tsconfig.prod.json && pm2 restart ecosystem.config.js --env production"
    },
    "repository": {
        "type": "git",
        "url": "train"
    },
    "author": "115",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.7.4",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "node-postgres": "^0.6.2",
        "pg": "^8.12.0",
        "pm2": "^5.4.2",
        "ts-node": "^10.9.2",
        "typescript": "^5.5.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.6",
        "@types/pg": "^8.11.6",
        "jest": "^29.7.0",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.2",
        "ts-node-dev": "^2.0.0"
    }
}
